{
  "name": "example-automatic-lighthouse-cd-pipeline",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist && cp -R ./src/public ./dist/public",
    "dev": "NODE_ENV=development DEBUG=example-automatic-lighthouse-cd-pipeline:* npm-run-all server",
    "production": "NODE_ENV=production npm-run-all clean build server:production",
    "server": "babel-node ./src/bin/www",
    "server:production": "node ./dist/bin/www",
    "start": "npm run production",
    "test": "npm run test:unit",
    "test:unit": "./node_modules/.bin/mocha --recursive src/test/unit --require @babel/register",
    "watch": "nodemon"
  },
  "dependencies": {
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "~1.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0"
  }
}
